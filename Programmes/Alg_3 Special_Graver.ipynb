    "def SpecGr_Com(Gr_1,m,n):\n",
    "    A = [0]*m\n",
    "    SpecGr = []\n",
    "    for k in range(1,n+1):\n",
    "        fro l in Gr_1:\n",
    "            temp = A+[0]*(k-1)+l+[0]*(n-k)\n",
    "            SpecGr.append(temp)\n",
    "    return SpecGr"
   ]
